<body>
    <div id="loading">
        <div class="spinner">
            <div class="bounce1"></div>
            <div class="bounce2"></div>
            <div class="bounce3"></div>
        </div>
    </div>
    <style type="text/css">
        html, body {
            height: 100%;
            background: #fff;
            overflow: hidden;
        }
    </style>

    <script type="text/javascript" src="@Url.Content("~/Content/assets/widgets/wow/wow.js")"></script>
    <script type="text/javascript">
        /* WOW animations */

        wow = new WOW({
            animateClass: 'animated',
            offset: 100
        });
        wow.init();
    </script>
    
    <img src="@Url.Content("~/Content/assets/image-resources/blurred-bg/blurred-bg-3.jpg")" class="login-img wow fadeIn" alt="">
    <div class="center-vertical">
        <div class="center-content row">
            <div class="col-md-3 center-margin">
                @using (Html.BeginForm())
                {
                    @Html.AntiForgeryToken()
                    <div class="content-box wow bounceInDown modal-content">
                        <h3 class="content-box-header content-box-header-alt bg-default">
                            <span class="icon-separator">
                                <i class="glyph-icon icon-cog"></i>
                            </span>
                            <span class="header-wrapper">
                                Members area
                                <small>Login to your account.</small>
                            </span>
                            <span class="header-buttons" style="display:none">
                                <a href="#" class="btn btn-sm btn-primary" title="">Sign Up</a>
                            </span>
                        </h3>

                        <div class="content-box-wrapper">
                            <div class="form-group">
                                <div class="input-group">
                                    @*<input type="email" class="form-control" id="exampleInputEmail1" placeholder="Enter email">*@

                                    @Html.EditorFor(model => model.Username, new { htmlAttributes = new { @class = "form-control", placeholder = "Enter Username" } })
                                    @Html.ValidationMessageFor(model => model.Username, "", new { @class = "text-danger" })

                                    <span class="input-group-addon bg-blue">
                                        <i class="glyph-icon icon-envelope-o"></i>
                                    </span>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="input-group">
                                    @*<input type="password" class="form-control" id="exampleInputPassword1" placeholder="Password">*@
                                    @Html.EditorFor(model => model.Password, new { htmlAttributes = new { @class = "form-control", @type = "password", placeholder = "Password" } })
                                    @Html.ValidationMessageFor(model => model.Password, "", new { @class = "text-danger" })
                                    <span class="input-group-addon bg-blue">
                                        <i class="glyph-icon icon-unlock-alt"></i>
                                    </span>
                                </div>
                            </div>
                            <div class="form-group">
                                <a href="/Admin/Forgot" title="Recover password">Forgot Your Password?</a>
                            </div>
                            <button class="btn btn-success btn-block" type="submit">Sign In</button>
                        </div>

                    </div>
                }
            </div>
        </div>
    </div>

    <!-- WIDGETS -->
    <script type="text/javascript" src="@Url.Content("~/Content/assets/bootstrap/js/bootstrap.js")" )"></script>
    <!-- Bootstrap Progress Bar -->
    <script type="text/javascript" src="@Url.Content("~/Content/assets/widgets/progressbar/progressbar.js")"></script>
